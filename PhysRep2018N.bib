arxivId = {1602.05280},
author = {Yu, Zu-Guo and Zhang, Huan and Huang, Da-Wen and Lin, Yong and Anh, Vo},
doi = {10.1088/1742-5468/2016/03/033206},
eprint = {1602.05280},
file = {:Users/yongzou/Downloads/PhysRepTimeSeriesNetworks/Visibility Graphs/Yu{\_}2016{\_}J.{\_}Stat.{\_}Mech.{\_}2016{\_}033206.pdf:pdf},
issn = {1742-5468},
