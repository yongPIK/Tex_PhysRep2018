keywords = {Earthquakes,complexity,nonlinear dynamics,seismic signals,statistical methods},
pages = {95--139},
publisher = {Elsevier},
title = {{4 - Fractal, Informational and Topological Methods for the Analysis of Discrete and Continuous Seismic Time Series: An Overview}},

year = {2018}
}
