journal = {Environ. Plan. B Plan. Des.},
month = {feb},
number = {1},
pages = {103--121},
title = {{From Isovists to Visibility Graphs: A Methodology for the Analysis of Architectural Space}},

volume = {28},
year = {2001}
}
