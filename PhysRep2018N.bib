@article{Yu2016,
abstract = {Many studies have shown that additional information can be gained on time series by investigating their associated complex networks. In this work, we investigate the multifractal property and Laplace spectrum of the horizontal visibility graphs (HVGs) constructed from fractional Brownian motions. We aim to identify via simulation and curve fitting the form of these properties in terms of the Hurst index {\$}H{\$}. First, we use the sandbox algorithm to study the multifractality of these HVGs. It is found that multifractality exists in these HVGs. We find that the average fractal dimension {\$}\backslashlangle D(0)\backslashrangle{\$} of HVGs approximately satisfies the prominent linear formula {\$}\backslashlangle D(0)\backslashrangle = 2 - H{\$}; while the average information dimension {\$}\backslashlangle D(1)\backslashrangle{\$} and average correlation dimension {\$}\backslashlangle D(2)\backslashrangle{\$} are all approximately bi-linear functions of {\$}H{\$} when {\$}H\backslashge 0.15{\$}. Then, we calculate the spectrum and energy for the general Laplacian operator and normalized Laplacian operator of these HVGs. We find that, for the general Laplacian operator, the average logarithm of second-smallest eigenvalue {\$}\backslashlangle \backslashln (u{\_}2) \backslashrangle{\$}, the average logarithm of third-smallest eigenvalue {\$}\backslashlangle \backslashln (u{\_}3) \backslashrangle{\$}, and the average logarithm of maximum eigenvalue {\$}\backslashlangle \backslashln (u{\_}n) \backslashrangle{\$} of these HVGs are approximately linear functions of {\$}H{\$}; while the average Laplacian energy {\$}\backslashlangle E{\_}{\{}nL{\}} \backslashrangle{\$} is approximately a quadratic polynomial function of {\$}H{\$}. For the normalized Laplacian operator, {\$}\backslashlangle \backslashln (u{\_}2) \backslashrangle{\$} and {\$}\backslashlangle \backslashln (u{\_}3) \backslashrangle{\$} of these HVGs approximately satisfy linear functions of {\$}H{\$}; while {\$}\backslashlangle \backslashln (u{\_}n) \backslashrangle{\$} and {\$}\backslashlangle E{\_}{\{}nL{\}} \backslashrangle{\$} are approximately a 4th and cubic polynomial function of {\$}H{\$} respectively.},
